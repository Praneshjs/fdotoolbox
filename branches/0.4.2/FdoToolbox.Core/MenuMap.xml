<?xml version="1.0" encoding="utf-8" ?>
<!-- 
Menu definition file. 

Edit this file to customise the application menu.

Commands that don't exist or cannot be executed from the user interface 
won't be shown in the menu.

DO NOT DELETE THIS FILE!!!
-->
<MenuMap>
	<Menu name="File">
		<Command name="connect" />
		<Command name="mkdstore" />
		<Separator/>
		<Command name="quit" />
	</Menu>
	<Menu name="Tools">
		<SubMenu name="FDO">
			<Command name="regprovider" />
			<Command name="unregprovider" />
		</SubMenu>
		<Command name="csmanager" />
		<Command name="extload" />
	</Menu>
	<Menu name="Express">
		<SubMenu name="Connect" resource="database_connect">
			<Command name="sdfconnect" displayName="SDF" />
			<Command name="shpconnect" displayName="SHP" />
			<Command name="shpdirconnect" displayName="SHP (directory)" />
		</SubMenu>
		<SubMenu name="Create" resource="database_add">
			<Command name="sdfcreate" displayName="SDF" />
			<Command name="shpcreate" displayName="SHP" />
		</SubMenu>
		<SubMenu name="Bulk Copy" resource="table_go">
			<Command name="sdf2sdf" />
			<Command name="shp2shp" />
			<Command name="sdf2shp" />
			<Command name="shp2sdf" /> 
		</SubMenu>
	</Menu>
	<Menu name="View">
		<Command name="showobjectexplorer" />
		<Command name="showconsole" />
	</Menu>
	<Menu name="Help">
		<Command name="help" />
		<Command name="about" />
	</Menu>
</MenuMap>