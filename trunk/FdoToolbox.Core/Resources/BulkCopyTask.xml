<?xml version="1.0" encoding="utf-8" ?>
<BulkCopyTask name="{0}">
	<!-- Source connection information -->
	<Source>
		<Provider>{1}</Provider>
		<ConnectionString>{2}</ConnectionString>
		<Schema>{3}</Schema>
		<SpatialContextList>
			{4}		  
		</SpatialContextList>
	</Source>
	<!-- Target connection information -->
	<Target>
		<Provider>{5}</Provider>
		<ConnectionString>{6}</ConnectionString>
		<Schema>{7}</Schema>
	</Target>
	<!-- Class mappings -->
	<ClassMappings>
		{8}
	</ClassMappings>
	<!-- Global options -->
	<BulkCopyOptions>
		<!-- 
		If true will copy all the spatial contexts defined
		in the <SpatialContextList> element
	    -->
		<CopySpatialContexts>{9}</CopySpatialContexts>
		<!--
		If true will try to convert the source property type to
		the target property type. Currently not supported.
		-->
		<CoerceDataTypes>{10}</CoerceDataTypes>
		<!--
		A global spatial filter (textual form) to apply to each
		class that is being copied. This is ignored if the class
		in question is not a Feature Class.
		-->
		<GlobalSpatialFilter>{11}</GlobalSpatialFilter>
	</BulkCopyOptions>
</BulkCopyTask>