<?xml version="1.0" encoding="utf-8" ?>
<!-- 
Object Explorer menu definition file. 

Edit this file to customise the Object Explorer menus.

Commands that don't exist or cannot be executed from the user interface 
won't be shown in the menu.

DO NOT DELETE THIS FILE!!!
-->
<ObjectExplorer>
	<Toolbar>
		<Menu name="New" resource="page_white">
			<SubMenu name="Connection" resource="database_connect">
				<Command name="sdfconnect" displayName="SDF" />
				<Command name="shpconnect" displayName="SHP" />
				<Command name="shpdirconnect" displayName="SHP (directory)" />
				<Command name="connect" displayName="Custom" />
			</SubMenu>
			<SubMenu name="Task" resource="application_go">
				<Command name="createbcp" />
			</SubMenu>
			<SubMenu name="Data Store" resource="database">
				<Command name="sdfcreate" displayName="SDF" />
				<Command name="shpcreate" displayName="SHP" />
				<Command name="mkdstore" displayName="Custom" />
			</SubMenu>
		</Menu>
		<Command name="extload" />
	</Toolbar>
	<ContextMenus>
		<Connections>
			<Menu name="New Connection" resource="database_connect">
				<Command name="sdfconnect" displayName="SDF" />
				<Command name="shpconnect" displayName="SHP" />
				<Command name="shpdirconnect" displayName="SHP (directory)" />
				<Command name="connect" displayName="Custom" />
			</Menu>
			<Command name="loadconn" />
		</Connections>
		<Tasks>
			<Menu name="New" resource="application_go">
				<Command name="createbcp" />
			</Menu>
			<Command name="loadtask" />
		</Tasks>
		<Modules>
			<Command name="extload" />
		</Modules>
		<SelectedConnection>
			<Command name="datapreview" />
			<Command name="manschema" />
			<Command name="mansc" />
			<Separator/>
			<Command name="renameconn" />
			<Command name="saveconn" />
			<Separator/>
			<Command name="saveschema" />
			<Command name="loadschema" />
			<Separator/>
			<Command name="removeconn" />
		</SelectedConnection>
		<SelectedTask>
			<Command name="exectask" />
			<!--
			<Command name="edittask" />
			-->
			<Command name="deletetask" />
			<Command name="savetask" />
		</SelectedTask>
		<SelectedModule>
			<Command name="modinfo" />
		</SelectedModule>
	</ContextMenus>
</ObjectExplorer>